{
  "name": "qwik-app",
  "description": "File based routing and MDX support Vite.js tooling. Express.js server. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.express.tsx",
    "dev": "npm run dev.ssr",
    "dev.client": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "serve": "node server/entry.express",
    "start": "npm run dev",
    "typecheck": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "latest",
    "@builder.io/qwik-city": "latest",
    "@types/eslint": "latest",
    "@types/express": "latest",
    "@types/node": "latest",
    "@types/react-linkify": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "5.34.0",
    "@typescript-eslint/parser": "5.34.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.22.0",
    "eslint-plugin-qwik": "0.0.108",
    "express": "latest",
    "node-fetch": "latest",
    "postcss": "latest",
    "prettier": "2.7.1",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "3.5.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "type": "module",
  "dependencies": {
    "linkify-plugin-hashtag": "^4.0.0",
    "linkify-plugin-mention": "^4.0.0",
    "linkify-react": "^4.0.0",
    "linkify-string": "^4.0.0",
    "linkifyjs": "^4.0.0",
    "react-linkify": "^1.0.0-alpha",
    "react-mentions": "^4.4.7",
    "react-modal": "^3.15.1",
    "react-overlays": "^5.2.1",
    "react-pure-modal": "^2.2.0",
    "reactjs-popup": "^2.0.5"
  }
}
