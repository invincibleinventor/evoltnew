{
  "name": "qwik-app",
  "description": "File based routing and MDX support Vite.js tooling. Express.js server. Prettier code formatter. Tailwind CSS framework.",
  "scripts": {
    "build": "npm run build.client && npm run build.ssr && npm run build.netlify",
    "build.cf": "vite build --ssr src/entry.cloudfare.tsx",
    "build.client": "vite build",
    "build.netlify": "vite build --ssr src/entry.netlify-edge.tsx",
    "build.server": "vite build -c adapters/netlify-edge/vite.config.ts",
    "build.ssr": "vite build --ssr",
    "deploy": "netlify deploy --build",
    "dev": "npm run dev.ssr",
    "dev.client": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --force --mode ssr",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "netlify.serve": "netlify dev",
    "serve": "node server/entry.express",
    "servessr": "node server/entry.ssr",
    "start": "npm run dev",
    "typecheck": "tsc --incremental --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "1.2.10",
    "@builder.io/qwik-city": "^1.2.10",
    "@builder.io/qwik-react": "0.5.0",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@mui/material": "5.14.7",
    "@netlify/edge-functions": "^2.0.0",
    "@netlify/vite-plugin-netlify-edge": "1.1.2",
    "@types/eslint": "latest",
    "@types/express": "latest",
    "@types/node": "latest",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-linkify": "^1.0.1",
    "@types/react-mentions": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "autoprefixer": "10.4.15",
    "eslint": "8.48.0",
    "eslint-plugin-qwik": "1.2.10",
    "express": "latest",
    "netlify-cli": "^15.0.0",
    "node-fetch": "latest",
    "postcss": "latest",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "latest",
    "typescript": "latest",
    "vite": "latest",
    "vite-tsconfig-paths": "4.2.0"
  },
  "engines": {
    "node": "^14.18.0 || >=16.0.0"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "type": "module",
  "dependencies": {
    "@emotion/server": "^11.11.0",
    "@supabase/supabase-js": "^2.33.1",
    "assert": "^2.0.0",
    "browserify-zlib": "^0.2.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "linkify-html": "^4.1.1",
    "linkify-react": "^4.1.1",
    "linkify-string": "^4.1.1",
    "linkifyjs": "^4.1.1",
    "netlify-cli": "^16.2.0",
    "punycode": "^2.3.0",
    "qwik": "^1.0.57",
    "react": "^18.2.0",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.2.0",
    "react-linkify": "^1.0.0-alpha",
    "react-mentions": "^4.4.10",
    "react-modal": "^3.16.1",
    "react-overlays": "^5.2.1",
    "react-pure-modal": "^2.2.5",
    "react-tabs": "^6.0.2",
    "react-tooltip": "^5.21.1",
    "reactjs-popup": "^2.0.5",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.1",
    "util": "^0.12.5"
  },
  "__qwik__": {
    "priority": 1,
    "viteConfig": {
      "imports": [
        {
          "defaultImport": "netlifyEdge",
          "importPath": "@netlify/vite-plugin-netlify-edge"
        }
      ],
      "qwikViteConfig": {
        "ssr": "{ outDir: 'netlify/edge-functions/entry.netlify-edge' }"
      },
      "vitePlugins": [
        "netlifyEdge({ functionName: 'entry.netlify-edge' })"
      ]
    }
  }
}
